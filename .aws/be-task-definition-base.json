{
  "family": "shortorial-be-task",
  "executionRoleArn": "dummy",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "containerDefinitions": [
    {
      "name": "shortorial-be-task",
      "image": "dummy",
      "essential": true,
      "portMappings": [
        {
          "name": "shortorial-be-task-8080-tcp",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "environment": [
        { "name": "MYSQL_URL", "value": "dummy" },
        { "name": "MYSQL_USER", "value": "dummy" },
        { "name": "MYSQL_PASSWORD", "value": "dummy" },
        { "name": "REDIS_URL", "value": "dummy" },
        { "name": "JWT_SECRET", "value": "dummy" },
        { "name": "ACCESS_KEY", "value": "dummy" },
        { "name": "SECRET_KEY", "value": "dummy" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/shortorial-be-task",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
